{
  "manifest_version": 3,
  "name": "Ad_del",
  "version": "0.1.0",
  "description": "hdy ad delete",
  "icons": {
    "16": "icons/icon_16.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Ad_del",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab", "scripting", "storage"
  ],
  "host_permissions": ["<all_urls>"] 
}